{
  "name": "server",
  "version": "1.0.0",
  "description": "server for SwapMeet",
  "main": "index.js",
  "scripts": {
    "test:api": "jest --detectOpenHandles --testPathPattern=tests/api",
    "cypress:test": "npx cypress run --config-file tests/cypress",
    "cypress:dev": "npx cypress open --browser chrome --config-file tests/cypress",
    "test:server:seed:user" : "node -e \"require('./app/database').seedUser()\"",
    "test:server:clear" : "node -e \"require('./app/database').clearDatabase()\"",
    "test:server:connect" : "node -e \"require('./app/database').connect()\"",
    "start_local": "node index.js && set BASE_URL=\"http://localhost:8080\"",
    "start_server": "node index.js && set BASE_URL=\"http://autafi.com\"",
    "start": "node ./bin/www"
  },
  "author": "peburney",
  "license": "ISC",
  "dependencies": {
    "@shelf/jest-mongodb": "^2.2.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mongodb-memory-server": "^8.5.2",
    "mongoose": "^6.1.1",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.2",
    "typescript": "^4.5.3",
    "vue-cookies": "^1.7.4"
  },
  "devDependencies": {
    "cypress": "^9.6.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2"
  }
}
